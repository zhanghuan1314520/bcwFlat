<view class="content">
  <view class="list" bindtap="childrens">
    <view class="list_right">
      <view>
        <image class="img1" src="../../images/my_01.png" />
      </view>
      <view class="title">我的孩子</view>
    </view>
    <view class="list_left">
      <image class="img1" src="../../images/jt_left.png" />
    </view>
  </view>
  <view class="list" bindtap="information_updatas">
    <view class="list_right">
      <view>
        <image class="img1" src="../../images/my_02.png" />
      </view>
      <view class="title">家长设置</view>
    </view>
    <view class="list_left">
      <image class="img1" src="../../images/jt_left.png" />
    </view>
  </view>
  <view class="list" bindtap="abouts">
    <view class="list_right">
      <view>
        <image class="img1" src="../../images/my_03.png" />
      </view>
      <view class="title">关于我们</view>
    </view>
    <view class="list_left">
      <image class="img1" src="../../images/jt_left.png" />
    </view>
  </view>
  <view class="list" bindtap="feedbacks">
    <view class="list_right">
      <view>
        <image class="img1" src="../../images/my_04.png" />
      </view>
      <view class="title">问题反馈</view>
    </view>
    <view class="list_left">
      <image class="img1" src="../../images/jt_left.png" />
    </view>
  </view>
  <view class="list" bindtap="quit">
    <view class="list_right">
      <view>
        <image class="img1" src="../../images/my_05.png" />
      </view>
      <view class="title">退出账号</view>
    </view>
    <view class="list_left">
      <image class="img1" src="../../images/jt_left.png" />
    </view>
  </view>
</view>