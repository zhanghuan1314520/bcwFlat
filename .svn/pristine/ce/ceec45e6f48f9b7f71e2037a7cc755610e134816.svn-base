.card{
  margin: 20rpx 20rpx 30rpx 20rpx;
  display: flex;
  padding: 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.card_left{
  display: flex;
  flex: 10;
  padding: 0 15rpx;
}
.card_right{
  flex: 1;
  align-items: center;  
  padding: 38rpx 0;
}
.img{
  width: 120rpx;
  height: 120rpx;
}
.left_one{
  height: 120rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.left_two{
  flex: 8;
  height: 120rpx;
  align-items: center;
  margin-left: 20rpx;
}
.title{
  margin: 10rpx 0;
}
.tip{
  margin-top: 30rpx;
  font-size: 25rpx;
  display: flex;
}
.name{
  color: aquamarine;
}
.m{
  margin-left: 15rpx;
}

.card1{
  margin: 20rpx;
  display: flex;
  flex-direction: column;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.card_left1{
  display: flex;
  flex: 3;
  padding: 10rpx  15rpx;
}
.card_right1{
  flex: 1;
  align-items: center;  
  padding: 35rpx 0;
}
.btn{
  color: aquamarine;
  font-size: 25rpx;
  text-align: center;
  padding: 10rpx;
  border-radius: 20rpx;
  border: 1px solid springgreen;
}
.card_list_one{
  display: flex;
border-bottom: 1px solid #ccc;
}
.card_list_two{
  display: flex;
  margin: 10rpx 0;
  padding: 10rpx 0;
}
.list_left{
  flex: 6;
  align-self: center;
  font-weight: bold;
  font-size: 28rpx;
}
.list_right{
  /* flex: 1; */
  align-self: center;
}
.time{
  display: flex;
  border: 1px solid #804CF7;
  padding: 10rpx;
  margin: 20rpx 0;
  color: #804CF7;
  font-size: 20rpx;
  border-radius: 10rpx;
}
.time_title{
  flex: 1;
  padding: 0 20rpx;
  align-self: center;
}
.time_times{
  flex: 5;
 text-align: right;
  align-self: center;
}
.img1{
  width: 35rpx;
  height: 20rpx;
}
.times{
  flex: 1;
  display: flex;
}
.time_two{
  flex: 1;
}
