/* 内容部分开始 */
.content {
  background-color: #fff;
  border-radius: 10rpx;
  margin: 30rpx;
}
.reg {
  padding: 10rpx 0;
}
.img {
  width: 30rpx;
  height: 30rpx;
}
.content_form {
  display: flex;
  padding:20rpx;
  border-bottom: 1px solid #ccc;
}
.content_form:last-child {
  border: none
}

.content_form_one {
  padding-top: 10rpx;
}

.content_form_two {
  flex: 10;
  font-size: 25rpx;
  margin-left: 20rpx;
  align-self: center;
  padding-right: 50rpx;
  letter-spacing:5rpx;
}

.content_form_one1 {
  padding-top: 15rpx;
}

.content_form_two1 {
  display: flex;
  flex: 10;
  font-size: 25rpx;
  margin-left: 20rpx;
  align-self: center;
}

/* 内容部分结束*/
/* 登录按钮开始 */
.btn {
  margin: 0 80rpx;
  padding: 20rpx 0;
}

/* 登录按钮结束 */
.button {
  width: 200rpx;
  font-size: 28rpx;
  background: #4268EF;
  color: #FFFFFF;
}

.content_form_two_left {
  flex: 1;
}

.content_form_two_right {
  flex: 3;
  padding-right: 60rpx;
  margin-top: 10rpx;
  letter-spacing:5rpx;
}