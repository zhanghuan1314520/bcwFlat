/* pages/time/index.wxss */
.content{
  margin: 28rpx;
}
.nav{
  background-color: #fff;
  padding:27rpx;
  margin-bottom: 20rpx;
}
.nav_top{
  border-bottom: 1px solid #dcdcdc;
  padding-bottom: 10rpx;
}
.top_list{
  display: flex;
  align-items: center;
  padding: 10rpx 0;
}
.top_title{
  flex: 10;
  color:#1F1F1F;
  padding: 10rpx 0;
  font-weight: bold;
  font-size: 32rpx;
}
.top_btn{
  flex: 2;
}
.tip{
  font-size: 20rpx;
  padding: 10rpx 0;
  color: #666;
}
.nav_button{
  border-radius: 10rpx;
  margin-top: 20rpx;
}
.name{
  flex: 1;
  color: #804CF7;
  font-weight: bold;
  font-size: 32rpx;
}
.time{
  margin-top: 20rpx;
  display: flex;
}
.time_one{
  flex: 1;
}
.time_two{
  flex: 1;
}

.one{
  align-self: center;
  color: #804CF8;
  border: 1px solid #804CF7;
  border-radius: 10rpx;
  padding: 10rpx 0;
  font-size: 30rpx;
  text-align: center;
}

.br{
  color:#804CF8;
  padding: 5rpx;
}