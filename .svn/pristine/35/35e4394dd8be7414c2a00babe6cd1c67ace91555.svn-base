page {
  width: 100%;
  height: 100%;
}
/* 内容部分开始 */
.content{
  background-color: #fff;
  border-radius: 10rpx;
  margin: 30rpx;
  padding: 20rpx;
}
 .reg{
   padding: 10rpx 20rpx;
 }
 .img{
   width: 30rpx;
   height: 30rpx;
 }
 .content_form{
   display: flex;
   padding: 15rpx 0;
   border-bottom: 1px solid #ccc;
 }
.content_form:last-child{border:none} 
 .content_form_one{
   flex: 1;
   padding: 7rpx 0;
 }
 .content_form_two{
  flex: 10;
  font-size: 25rpx;
}
/* 内容部分结束*/
/* 登录按钮开始 */
.btn{
  margin:0 80rpx;
  border-radius: 50%;
  padding: 20rpx 0;
}
/* 登录按钮结束 */
/* 验证码开始 */
.mod{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}

.t-name{
  background-color: rgb(254,254,254);
  box-sizing: border-box;
  flex: 10;
  font-size: 25rpx;
}
.logbook {
  font-size: 24rpx;
  width: 150rpx;
  text-align: right;
  font-weight: bold;
}

.getkey {
  width: 200rpx;
  position: absolute;
  right: 0rpx;
  text-align: center;
  color: #377FF6;
  font-size: 24rpx;
  border-left: 1px solid #f0f0f0;
}
/* 验证码结束 */
 