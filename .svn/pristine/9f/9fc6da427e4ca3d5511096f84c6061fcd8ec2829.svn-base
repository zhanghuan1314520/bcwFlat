.content {
  padding: 0 255rpx;
  background-color: #4268EF;
}

.nav {
  background-color: rgb(118, 160, 238);
  padding: 6rpx 0;
  border-radius: 30rpx;
  border: 1px solid #fff;
  font-size: 28rpx;
}

.time {
  display: flex;
  overflow: hidden;
}

.img {
  width: 30rpx;
  height: 30rpx;
}

.time_one {
  flex: 2;
  text-align: center;
  align-self: center;
  color: #fff;
}

.time_two {
  flex: 1;
  padding-top: 12rpx;
}

.times {
  padding: 20rpx 0;
  color: #fff;
  text-align: center;
  font-size: 23rpx;
  background-color: #4268EF;
}

.times_title {
  margin-top: 20rpx;
}

.times_tip {
  display: flex;
  padding: 20rpx 270rpx;
}

/* 图表部分开始 */
.chart {
  flex-direction: row;
  padding: 50rpx 20rpx;
  background-color: #4268EF;
}

.chart_item {
  display: flex;
  align-items: flex-end;
  padding: 0 20rpx;
}

.chart_item_one {
  flex: 1;
  height: 120rpx;
  margin: 0 20rpx;
  text-align: center;
  border-radius: 10rpx;
  align-items: center;
  background: #ED7A6B;
}

.chart_item_two {
  flex: 1;
  height: 160rpx;
  margin: 0 20rpx;
  text-align: center;
  border-radius: 10rpx;
  background: #92E6AA;
}

.chart_item_three {
  flex: 1;
  height: 100rpx;
  text-align: center;
  margin: 0 20rpx;
  border-radius: 10rpx;
  background: #F8D693;
}

.chart_item_m {
  position: relative;
  top: -45rpx;
  color: #fff;
  font-size: 20rpx;
}

.chart_item_name {
  color: #000;
  font-size: 22rpx;
  margin-top: 45rpx;
}

.chart_item_m1 {
  position: relative;
  top: -45rpx;
  color: #fff;
  font-size: 20rpx;
}

.chart_item_name1 {
  color: #000;
  font-size: 22rpx;
  margin-top: 85rpx;
}

.chart_item_m2 {
  position: relative;
  top: -45rpx;
  color: #fff;
  font-size: 20rpx;
}

.chart_item_name2 {
  color: #000;
  font-size: 22rpx;
  margin-top: 25rpx;
}

/* bg */
.bg {
  margin-top: -20rpx;
  width: 100%;
  height: 100rpx;
}

.bg_img {
  width: 100%;
  height: 100rpx;
}

/* bg */

/* 图表部分结束 */

/* footer部分开始*/
.footer {
  margin: 20rpx 20rpx 0 20rpx;
  padding: 20rpx 20rpx 0 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
}

.top {
  display: flex;
  flex-direction: row;
}

.tab1 {
  width: 200rpx;
  color: #999;
  font-size: 28rpx;
  padding: 15rpx 10rpx;
  border: 1px solid #ccc;
  border-radius: 30rpx;
}

.tab2 {
  width: 200rpx;
  padding: 10rpx;
  color: #999;
  font-size: 28rpx;
  border: 1px solid #ccc;
  border-radius: 30rpx;
  margin-left: 10rpx;
}

.top {
  padding: 20rpx;
  text-align: center;
}

.top .active {
  color: #fff;
  background-color: #4268EF;
  border: 1px solid #4268EF;
}

.record {
  padding: 20rpx 20rpx 0 20rpx;
}

.statistics {
  padding: 20rpx;
}

.table {
  border: 0px solid darkgray;
}
/* footer部分结束*/
.table_name {
  display: flex;
  height: 60rpx;
  font-size: 25rpx;
  background-color: #F0F0F0;
  align-items: center;
  text-align: center;
}

.table_name_one {
  flex: 1;
}

.table_name_two {
  flex: 2;
}

.table_name_three {
  flex: 1;
}

.table_item {
  display: flex;
  text-align: center;
 padding: 12rpx 0;
  align-items: center;
  font-size: 22rpx;
  color: #999;
}

.table_item_one {
  flex: 1;
}

.table_item_two {
  flex: 2;
}

.table_item_three {
  flex: 1;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.table_item_two1_img{
  width: 30rpx;
  height: 30rpx;
  border-radius: 2rpx;
}
.table_item_two1 {
  flex: 2;
  display: flex;
}
.table_item_two1_right{
  flex: 1;
  text-align: right;
}
.table_item_two1_left{
  flex: 2;
  text-align: left;
  padding-left: 10rpx;
}
.number{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50rpx;
 margin: 0 70rpx;
  background-color: rgb(236, 161, 161);
  color: #fff;
}