<view class="content">
  <form class="page__bd" bindsubmit="formSubmit">
    <view class="item">
      <view class="item_right">MAC</view>
      <view class="item_left">
        <input class="weui-input" name="input" auto-focus placeholder="20:72:0D：39:01:3E" />
      </view>
    </view>
    <view class="item">
      <view class="item_right">姓名</view>
      <view class="item_left">
        <input class="weui-input" name="username" placeholder="请输入姓名" />
      </view>
    </view>
    <view class="item">
      <view class="item_right">性别</view>
      <view class="item_left">
        <view>
          <picker name="areaPicker" bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
            <view class="picker">{{array[index]}}</view>
          </picker>
        </view>
      </view>
      <view class="time_two">
        <image class="img1" src="../../../images/jt.png" />
      </view>
    </view>
    <view class="item">
      <view class="item_right">学号</view>
      <view class="item_left">
        <input class="weui-input" name="input" placeholder="请输入学号" />
      </view>
    </view>
    <view class="btn">
      <van-button round type="info" block form-type="submit">提交</van-button>
    </view>
  </form>
</view>