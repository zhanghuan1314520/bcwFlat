<view>
  <map id="myMap" longitude="120.2985" latitude="30.41875" scale="16"
    bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap"
    bindregionchange="regionchange" show-location style="width: 100%; height:{{MapHeight}};">
    <cover-image class="back_img" src="../../images/map.png"  bindtap="moveTolocation"></cover-image>
  </map>
</view>

